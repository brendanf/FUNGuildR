year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{FUNGuildR}: Look Up Guild Information for Fungi",
         author = c(person("Brendan", "Furneaux"),
                    person("Zewei", "Song")),
         year = year,
         note = note,
         url = "https://CRAN.R-project.org/package=FUNGuildR")
